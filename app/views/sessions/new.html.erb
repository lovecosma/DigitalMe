<div class="container">
  <div class="white-text wrapper deep-purple">
    <h1>Login</h1>
  </div>
  <div class="">
    <%= form_for @user, url: login_path do |f| %>
    <%= f.label :username %><br>
    <%= f.text_field :username %><br><br>
    <%= f.label :password %><br>
    <%= f.password_field :password %><br><br>
    <%= f.submit "login"%>
    <% end %>
  </div>

  <%= link_to "Sign In with Github", "/auth/github" %>

</div>
