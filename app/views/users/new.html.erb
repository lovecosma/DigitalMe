<h1>Create Account</h1>
<div class="">
  <%= form_for @user, url: signup_path do |f| %>
  <%= f.label :username %><br>
  <%= f.text_field :username %><br><br>
  <%= f.label :password %><br>
  <%= f.password_field :password %><br><br>
  <%= f.submit "signup"%>
  <% end %>
</div>

<%= link_to "Sign Up with Github", "/auth/github" %>
