<div class="container">
  <div class="">
    <h1><%= @chart.full_name %></h1>
    <h4><%= "#{@chart.month}/#{@chart.day}/#{@chart.year} " %></h4>
    <%= link_to "edit", edit_user_chart_path(current_user, @chart) %>
    <%= form_tag user_chart_path(@chart.id), method: "delete" do %>
    <%= submit_tag "Delete" %>
  <% end %>
  </div>
  <br>
  <div>
    <h4> <%= "Birthday Number #{@chart.birthday_number}" %> <%= link_to "*", birthday_path(@chart.birthday) %></h4>
    <p><%= @chart.birthday.short_description %></p>
  </div>
  <div class="">
    <h4><%= "Life Path Number #{@chart.life_path_number}" %><%= link_to "*", life_path_path(@chart.life_path) %></h4>
    <p><%= @chart.life_path.short_description %></p>
  </div>
  <div class="">
    <h4> <%= "Soul Urge Number #{@chart.soul_urge_number}" %> <%= link_to "*", soul_urge_path(@chart.soul_urge) %></h4>
    <p><%= @chart.soul_urge.short_description %></p>
  </div>
  <div class="">
    <h4> <%="Soul Urge Challenge Number #{@chart.soul_urge_challenge_number}"%> <%= link_to "*", soul_urge_challenge_path(@chart.soul_urge_challenge) %></h4>
    <p><%= @chart.soul_urge_challenge.short_description %></p>
  </div>
  <div class="">
    <h4> <%="Expression Number #{@chart.expression_number}"%> <%= link_to "*", expression_path(@chart.expression) %></h4>
    <p><%= @chart.expression.short_description %></p>
  </div>
  <div class="">
    <h4> <%="Expression Challenge Number #{@chart.expression_challenge_number}"%> <%= link_to "*", expression_challenge_path(@chart.expression_challenge) %></h4>
    <p><%= @chart.expression_challenge.short_description %></p>
  </div>
  <div class="">
    <h4> <%="Personality Number #{@chart.personality_number}"%> <%= link_to "*", personality_path(@chart.personality) %> </h4>
    <p><%= @chart.personality.short_description %></p>
  </div>
  <div class="">
    <h4><%="Personality Challenge Number #{@chart.personality_challenge_number}"%><%= link_to "*", personality_challenge_path(@chart.personality_challenge) %></h4>
    <p><%= @chart.personality_challenge.short_description %></p>
  </div>

</div>
